<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
    
    <!-- ========== Data Source Configuration =============================== -->
    <data-sources />
    
    <!-- ========== Form Bean Definitions ================================== -->
    <form-beans>
        <form-bean name="geneexpressionForm" type="gov.nih.nci.nautilus.struts.form.GeneExpressionForm">
            <form-property name="geneList" type="java.lang.String" />
            <form-property name="goBiologicalProcess" type="java.lang.String" />
            <form-property name="tumorGrade" type="java.lang.String" />
            <form-property name="region" type="java.lang.String" />
            <form-property name="foldChangeValueDown" type="java.lang.String" initial="2" />
            <form-property name="cytobandRegion" type="java.lang.String" />
            <form-property name="cloneId" type="java.lang.String" />
            <form-property name="pathways" type="java.lang.String" />
            <form-property name="tumorType" type="java.lang.String" />
            <form-property name="arrayPlatform" type="java.lang.String" />
            <form-property name="cloneListFile" type="java.lang.String" />
            <form-property name="goCellularComp" type="java.lang.String" />
            <form-property name="goMolecularFunction" type="java.lang.String" />
            <form-property name="cloneListSpecify" type="java.lang.String" />
            <form-property name="goClassification" type="java.lang.String" />
            <form-property name="basePairEnd" type="java.lang.String" />
            <form-property name="chrosomeNumber" type="java.lang.String" />
            <form-property name="regulationStatus" type="java.lang.String" />
            <form-property name="foldChangeValueUnchange" type="java.lang.String" initial="2" />
            <form-property name="foldChangeValueUp" type="java.lang.String" initial="2" />
            <form-property name="geneType" type="java.lang.String" />
            <form-property name="foldChangeValueUDUp" type="java.lang.String" />
            <form-property name="resultView" type="java.lang.String" />
            <form-property name="geneFile" type="java.lang.String" />
            <form-property name="foldChangeValueUDDown" type="java.lang.String" initial="2" />
            <form-property name="geneGroup" type="java.lang.String" />
            <form-property name="cloneList" type="java.lang.String" />
            <form-property name="queryName" type="java.lang.String" />
            <form-property name="basePairStart" type="java.lang.String" />
        </form-bean>
        
        <form-bean name="comparitivegenomicForm" type="gov.nih.nci.nautilus.struts.form.ComparativeGenomicForm">
            <form-property name="geneList" type="java.lang.String" />
            <form-property name="tumorGrade" type="java.lang.String" />
            <form-property name="assayPlatform" type="java.lang.String" />
            <form-property name="region" type="java.lang.String" />
            <form-property name="cytobandRegion" type="java.lang.String" />
            <form-property name="snpList" type="java.lang.String" />
            <form-property name="cloneId" type="java.lang.String" />
            <form-property name="cnAmplified" type="java.lang.String" />
            <form-property name="tumorType" type="java.lang.String" />
            <form-property name="cloneListFile" type="java.lang.String" />
            <form-property name="snpListFile" type="java.lang.String" />
            <form-property name="cloneListSpecify" type="java.lang.String" />
            <form-property name="snpListSpecify" type="java.lang.String" />
            <form-property name="cnADAmplified" type="java.lang.String" />
            <form-property name="genomicTrack" type="java.lang.String" />
            <form-property name="basePairEnd" type="java.lang.String" />
            <form-property name="chrosomeNumber" type="java.lang.String" />
            <form-property name="cnADDeleted" type="java.lang.String" />
            <form-property name="cnUnchangeTo" type="java.lang.String" />
            <form-property name="alleleFrequency" type="java.lang.String" />
            <form-property name="geneType" type="java.lang.String" />
            <form-property name="validatedSNP" type="java.lang.String" />
            <form-property name="resultView" type="java.lang.String" />
            <form-property name="geneFile" type="java.lang.String" />
            <form-property name="snpId" type="java.lang.String" />
            <form-property name="cnDeleted" type="java.lang.String" />
            <form-property name="geneGroup" type="java.lang.String" />
            <form-property name="cnUnchangeFrom" type="java.lang.String" />
            <form-property name="cloneList" type="java.lang.String" />
            <form-property name="queryName" type="java.lang.String" />
            <form-property name="copyNumber" type="java.lang.String" />
            <form-property name="basePairStart" type="java.lang.String" />
        </form-bean>
		
		  <form-bean name="clinicaldataForm" type="gov.nih.nci.nautilus.struts.form.ClinicalDataForm">
            <form-property name="firstPresentation" type="java.lang.String" />
            <form-property name="recurrence" type="java.lang.String" />
            <form-property name="tumorGrade" type="java.lang.String" />
            <form-property name="radiation" type="java.lang.String" />
            <form-property name="radiationType" type="java.lang.String" initial="2" />          
            <form-property name="chemo" type="java.lang.String" />
            <form-property name="chemoType" type="java.lang.String" />
            <form-property name="tumorType" type="java.lang.String" />
            <form-property name="sugery" type="java.lang.String" />
            <form-property name="surgeryType" type="java.lang.String" />			
            <form-property name="survivalLower" type="java.lang.String" />
            <form-property name="survivalUpper" type="java.lang.String" />
			<form-property name="queryName" type="java.lang.String" /> 
            <form-property name="ageLower" type="java.lang.String" />
            <form-property name="ageUpper" type="java.lang.String" />
            <form-property name="genderType" type="java.lang.String" />  			       
        </form-bean>

	  	<form-bean name="refineQueryForm" type="gov.nih.nci.nautilus.struts.form.RefineQueryForm">
            <form-property name="queryName1" type="java.lang.String" />
            <form-property name="leftParen1" type="java.lang.String" />
            <form-property name="rightParen1" type="java.lang.String" />
            <form-property name="operatorType1" type="java.lang.String" />
            <form-property name="queryName2" type="java.lang.String" />
            <form-property name="leftParen2" type="java.lang.String" />
            <form-property name="rightParen2" type="java.lang.String" />
            <form-property name="operatorType2" type="java.lang.String" />
            <form-property name="queryName3" type="java.lang.String" />
            <form-property name="leftParen3" type="java.lang.String" />
            <form-property name="rightParen3" type="java.lang.String" />
            <form-property name="operatorType3" type="java.lang.String" />
            <form-property name="queryText" type="java.lang.String" />
            <form-property name="compoundView" type="java.lang.String" />
            <form-property name="resultsetName" type="java.lang.String" />
            <form-property name="method" type="java.lang.String" />
        </form-bean>

	  	<form-bean name="selectPresentationForm" type="gov.nih.nci.nautilus.struts.form.SelectPresentationForm">
            <form-property name="sampleListFrom" type="java.lang.String[]" />
            <form-property name="sampleListTo" type="java.lang.String" />
            <form-property name="geneFilterBy" type="java.lang.String" />
            <form-property name="geneViewBy" type="java.lang.String" />
            <form-property name="geneListFrom" type="java.lang.String" />
            <form-property name="geneListTo" type="java.lang.String" />
            <form-property name="method" type="java.lang.String" />
            <form-property name="listFrom" type="java.lang.String[]" />
            <form-property name="listTo" type="java.lang.String" />

        </form-bean>
        
            <!-- Logon form bean -->
                <form-bean      name="loginForm"
                    type="gov.nih.nci.nautilus.login.LoginForm"/>

		
    </form-beans>
    

    <!-- ========== Global Exception Definitions ============================== -->
    <global-exceptions />
    
  <!-- ========== Global Forward Definitions ============================== -->
  <global-forwards>
    <forward   name="logoff"               path="/logoff.do"/>
    <forward   name="logon"                path="/logon.jsp"/>
    <forward   name="success"              path="/mainMenu.jsp"/>
  </global-forwards>
    
    <!-- ========== Action Mapping Definitions =============================== -->
    <action-mappings>
    
    <action    path="/login"
               type="gov.nih.nci.nautilus.login.LoginAction"
               name="loginForm"
              scope="request"
              input="/login.jsp">
    <forward name="success" path="/home.do"/>
    <forward name="failure" path="/error.jsp"/>
    </action>

   <action    path="/geneExpression"
              forward="nautilus.geneExpression" />

	<action path="/geneexpression"
            input="nautilus.geneExpression"
            name="geneexpressionForm"
            type="gov.nih.nci.nautilus.struts.action.GeneexpressionAction"
            scope="request"
            validate="true">
         <forward name="advanceSearchMenu" path="nautilus.menu" />
         <forward name="backToGeneExp" path="nautilus.geneExpression" />
	</action>
	
   <action    path="/comparitiveGenomic"
              forward="nautilus.comparitiveGenomic" />

	<action path="/comparitivegenomic"
            input="nautilus.comparitiveGenomic"
            name="comparitivegenomicForm"
            type="gov.nih.nci.nautilus.struts.action.ComparativeGenomicAction"
            scope="request"
            validate="true">
         <forward name="advanceSearchMenu" path="nautilus.menu" />
         <forward name="backToCGH" path="nautilus.comparitiveGenomic" />
	</action>

	<action path="/menu"
		type="DoFirst"
		name="noForm">
		<forward name="success" path="nautilus.menu" />
	</action>

	 <action  path="/clinicalData"
              forward="nautilus.clinical" />

	<action path="/clinical"
            input="nautilus.clinical"
            name="clinicaldataForm"
            type="gov.nih.nci.nautilus.struts.action.ClinicalDataAction"
            scope="request"
            validate="true">
         <forward name="advanceSearchMenu" path="nautilus.menu" />
         <forward name="backToClinical" path="nautilus.clinical" />
	</action>

	<action path="/home"
		type="DoFirst">
		<forward name="success" path="nautilus.home" />
	</action>

   <action path="/refinecheck"
	       type="gov.nih.nci.nautilus.struts.action.RefineCheckAction">
         <forward name="success" path="/refine.do" />
         <forward name="failure" path="/menu.do" />
   </action>

   <action    path="/refine"
              forward="nautilus.refine" />

	<action path="/validatequery"
            input="nautilus.refine"
            name="refineQueryForm"
            type="gov.nih.nci.nautilus.struts.action.RefineQueryAction"
            scope="request"
            parameter="method"
            validate="true">
         <forward name="success" path="/presentation.do" />
         <forward name="displayQuery" path="nautilus.refine" />
         <forward name="failure" path="/refine.do" />
         
	</action>

   <action path="/compoundcheck"
	       type="gov.nih.nci.nautilus.struts.action.CompoundCheckAction"
           scope="request">
         <forward name="success" path="/presentation.do" />
         <forward name="failure" path="/refine.do" />
   </action>

   <action    path="/presentation"
              forward="nautilus.presentation" />

   <action path="/present"
           name="selectPresentationForm"
	       type="gov.nih.nci.nautilus.struts.action.SelectPresentDispatchAction"
	       input="nautilus.presentation" 
           scope="request"
           parameter="method">
         <forward name="success" path="/jsp/geneViewReport.jsp" />
         <forward name="backtopresent" path="/presentation.do" />
   </action>

    </action-mappings>
    
    <!-- ========== Controller Configuration ================================ -->
    <controller />
    
    <!-- ========== Message Resources Definitions ============================ -->
    <message-resources parameter="gov.nih.nci.nautilus.struts.ApplicationResources" />
    
    <!-- ========== Plug Ins Configuration ================================= -->
 <plug-in className="org.apache.struts.tiles.TilesPlugin" >
   <set-property property="definitions-config" value="/WEB-INF/tiles-defs.xml" />
   <set-property property="definitions-parser-validate" value="true" />
   <set-property property="definitions-debug" value="2" />
   <set-property property="definitions-parser-details" value="2" />
   <set-property property="moduleAware" value="true" />
 </plug-in>
 

</struts-config>

