        	 <param name="actionName">graph</param>
         </result>
         <result name="mismatch" type="redirectAction">
         	<param name="actionName">home</param>
         	<!--  param name="namespace">/</param> -->
        </result>
	</action>
	
	<action name="graph">
		<result type="tiles">nautilus.graph</result>
     </action>
           
    <action name="home"
		class="gov.nih.nci.rembrandt.web.struts2.action.DoFirst">
		<result name="success" type="tiles">nautilus.home</result>
	</action>
	
	<action name="menu">
